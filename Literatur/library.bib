keywords = {Gehan test,Harrell's C,Hodges-Lehmann median difference,Kendall's tau,ROC area,Somers' D,Theil median slope,Wilcoxon test,confidence intervals,nonparametric methods,rank correlation,rank-sum test},
number = {1},
pages = {45--64},
publisher = {StataCorp LP},
title = {{Parameters behind "nonparametric" statistics: Kendall's tau,Somers' D and median differences}},
url = {https://econpapers.repec.org/RePEc:tsj:stataj:v:2:y:2002:i:1:p:45-64},
volume = {2},
